}

td.mk_op /* order phone preview */ { 
    background-image: url(img/phone-sms.png); 
    background-repeat: no-repeat; 
    width: 110px; 
    height: 47px; 
    vertical-align: top;
    padding-top: 95px; 
    padding-left: 40px; 
    color: black; 
    font-size: 11px; 
    font-weight: bold; 
    font-family: Verdana, Arial, Helvetica, sans-serif; 
}
td.mk_oi /* order instruction */ { 
    width: auto; 
    height: auto; 
    padding-left: 10px; 
    color: black; 
    font-size: 11px; 
    font-family: Verdana, Arial, Helvetica, sans-serif; 
}
