	background-image: url(img/fr_l_01.gif); 
	height: auto;
	width: 25px;
}
td.mk_pls_fcc{ 
	background-image: url(img/fr_c_01.gif); 
	height: auto;
	width: auto;
}
td.mk_pls_fcr{ 
	background-image: url(img/fr_r_01.gif); 
	height: auto;
	width: 25px;
}
td.mk_pls_fbl{ 
