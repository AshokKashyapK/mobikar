	background-image: url(img/fr_rb_01.gif); 
	height: 25px;
	width: 25px;
}
a.mk_lst /* song title*/ { 
	background-image: url(img/ico.gif);
	background-repeat: no-repeat; 
	padding-left: 15px;
	color: #FFF; 
	text-decoration: underline; 
}
a.mk_lsa /* song artist */ { 
	color: #DDD; 
	text-decoration: underline; 
}
span.mk_lsh /* hypen */ { 
	background-color: #534c4d; 
	background-image: url(img/hyp.gif); 
	background-repeat: no-repeat; 
	background-position: center; 
	padding-right: 5px; 
	padding-left: 5px; 
}

