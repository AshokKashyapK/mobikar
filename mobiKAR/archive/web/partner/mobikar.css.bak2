	border-spacing: 0px; 
	border-collapse: collapse;
}
tr.mk_pls_row{ 
}
td.mk_pls_ftl{ 
	background-image: url(img/fr_lt_01.gif); 
	height: 25px;
	width: 25px;
}
td.mk_pls_ftc{ 
	background-image: url(img/fr_t_01.gif); 
	background-repeat: repeat-x; 
	height: 25px; 
	width: auto; 
}
td.mk_pls_ftr{ 
	background-image: url(img/fr_rt_01.gif); 
	height: 25px;
	width: 25px;
}

