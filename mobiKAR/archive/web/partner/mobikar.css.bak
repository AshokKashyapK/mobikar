div#mk_tab_nojs{
	background-color:#FFFF33;
	color:#003399;
	font-size:12px;
	font-family:Arial, Helvetica, sans-serif;
	font-weight:bold;
}
div.clr{
	clear: both;
}
div.mk_tab_menu{
	float:right;
	width:100%;
	background-color:#ffae00;
}

ul#mk_tab_menu{
	background-color:#ffae00;
	width: 400px;
	float: left;
	list-style: none;
	padding: 0;
	margin: 0;
	font-size: 0.8em;
}

ul#mk_tab_menu li{
	display: block;
	background-image: url(img/mobiKAR-button.png);
	background-position:center;
	background-repeat:no-repeat;
	float: left;
	margin: 0px;
	width: 100px;
	font-family:Verdana, Arial, Helvetica, sans-serif;
	font-weight:bold;
	font-size: 14px;
	line-height: 30px;
	white-space: nowrap;
	text-align:center;	

}

ul#mk_tab_menu li a
{
	width: 100%;
	display: block;
	text-decoration: underline;
	color:#FFF;
	background: transparent;
}

ul#mk_tab_menu li a:hover
{
	color: #ffcc00;
}

ul#mk_tab_menu li a#mk_tab_menu_sms{
	color:#FFF;
}
ul#mk_tab_menu li a#mk_tab_menu_cheaper{
	color:#FFF;
}
ul#mk_tab_menu li a#mk_tab_menu_targets{
	color:#FFF;
}
ul#mk_tab_menu li a#mk_tab_menu_help{
	color:#FFF;
}

div.mk_tab_body{
	width:100%;
}
div.mk_tab_order{
	background-color:#FFFF00;
	float:left;
	width:162px;
	margin-right: 15px;
	clear: left;
	display: block;
	text-align:center;
}
.mk_tab_content{
	float: none;
	overflow: auto;
	position: relative;
	visibility:visible;
	padding-right: 10px;
	padding-bottom: 10px;
}
.mk_tab_content_visible{
	float: left;
	overflow: visible;
	position: absolute;
	visibility:visible;
	top: 32px;
}
.mk_tab_content_visible ul li {
	font-weight:bold;
}
.mk_tab_content_hidden{
	float: left;
	overflow: visible;
	position: absolute;
	visibility: hidden;
}
