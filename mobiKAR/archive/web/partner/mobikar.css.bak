/* Order phone preview and info */
table.mk_o{
    margin: 0px; 
	padding: 0px; 
	border-width: 0px; 
	outline-width: 0px; 
	border-spacing: 0px; 
	border-collapse: collapse;
}
tr.mk_o /* frame */{ 
}

td.mk_op /* order phone preview */ { 
    background-image: url(img/phone-sms.png); 
    background-repeat: no-repeat; 
    width: 110px; 
    height: 47px; 
    vertical-align: top;
    padding-top: 95px; 
    padding-left: 40px; 
    color: black; 
    font-size: 11px; 
    font-weight: bold; 
    font-family: Verdana, Arial, Helvetica, sans-serif; 
}
td.mk_oi /* order instruction */ { 
    width: auto; 
    height: auto; 
    padding-left: 10px; 
    color: black; 
    font-size: 11px; 
    font-family: Verdana, Arial, Helvetica, sans-serif; 
}
div.oa /* operator available*/{
	background-color: #123;
	color: #aaa;
}
div.pr /* payment*/{
	background-color: #345;
	color: #ccc;
}
div.ph /* phones*/{
	background-color: #567;
	color: #888;
}
div.h /* help*/{
	background-color: #864;
	color: #aaa;
}


